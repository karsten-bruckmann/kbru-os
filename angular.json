{
  "version": 1,
  "projects": {
    "showcases-form": {
      "projectType": "application",
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      },
      "root": "apps/showcases/form",
      "sourceRoot": "apps/showcases/form/src",
      "prefix": "kbru",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/showcases/form",
            "index": "apps/showcases/form/src/index.html",
            "main": "apps/showcases/form/src/main.ts",
            "polyfills": "apps/showcases/form/src/polyfills.ts",
            "tsConfig": "apps/showcases/form/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/showcases/form/src/favicon.ico",
              "apps/showcases/form/src/assets"
            ],
            "styles": ["apps/showcases/form/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/showcases/form/src/environments/environment.ts",
                  "with": "apps/showcases/form/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "showcases-form:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "showcases-form:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "showcases-form:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/showcases/form/tsconfig.app.json",
              "apps/showcases/form/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/showcases/form/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/showcases/form/jest.config.js",
            "tsConfig": "apps/showcases/form/tsconfig.spec.json",
            "passWithNoTests": true,
            "setupFile": "apps/showcases/form/src/test-setup.ts"
          }
        }
      }
    },
    "form-effects": {
      "projectType": "library",
      "root": "libs/utils/form-effects",
      "sourceRoot": "libs/utils/form-effects/src",
      "prefix": "kbru",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:package",
          "options": {
            "tsConfig": "libs/utils/form-effects/tsconfig.lib.json",
            "project": "libs/utils/form-effects/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/utils/form-effects/tsconfig.lib.prod.json"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/utils/form-effects/tsconfig.lib.json",
              "libs/utils/form-effects/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/utils/form-effects/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/utils/form-effects/jest.config.js",
            "tsConfig": "libs/utils/form-effects/tsconfig.spec.json",
            "passWithNoTests": true,
            "setupFile": "libs/utils/form-effects/src/test-setup.ts"
          }
        },
        "publish": {
          "builder": "nx:run-commands",
          "options": {
            "commands": [
              {
                "command": "npm publish dist/libs/utils/form-effects --public"
              }
            ]
          }
        }
      },
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      }
    },
    "control-props": {
      "projectType": "library",
      "root": "libs/utils/control-props",
      "sourceRoot": "libs/utils/control-props/src",
      "prefix": "kbru",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:package",
          "options": {
            "tsConfig": "libs/utils/control-props/tsconfig.lib.json",
            "project": "libs/utils/control-props/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/utils/control-props/tsconfig.lib.prod.json"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/utils/control-props/tsconfig.lib.json",
              "libs/utils/control-props/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/utils/control-props/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/utils/control-props/jest.config.js",
            "tsConfig": "libs/utils/control-props/tsconfig.spec.json",
            "passWithNoTests": true,
            "setupFile": "libs/utils/control-props/src/test-setup.ts"
          }
        },
        "publish": {
          "builder": "nx:run-commands",
          "options": {
            "commands": [
              {
                "command": "npm publish dist/libs/utils/control-props --public"
              }
            ]
          }
        }
      },
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "schematics": {
    "@nrwl/angular:application": {
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "unitTestRunner": "jest"
    }
  },
  "defaultProject": "showcases-form"
}
